<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.asu.iproject.student.skillset.model.Skillset" />
	<class name="com.asu.iproject.student.skillset.model.impl.SkillsetImpl" table="Skillset_Skillset">
		<cache usage="read-write" />
		<id name="studentID" type="long">
			<generator class="assigned" />
		</id>
		<property name="asuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="skill1" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dateAdded" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>